{
  "name": "@cadview/react",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "description": "React wrapper for @cadview/core â€” CAD/DXF file viewer component",
  "author": "Wisnu Wicaksono",
  "keywords": ["cad", "dxf", "viewer", "react", "component", "canvas"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiscaksono/cadview.git",
